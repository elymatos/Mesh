{
  "name": "elymatos/mesh",
  "description": "Base classes for Recursive Cortical Network (RCN)",
  "license": "MIT",
  "keywords": [
    "mesh",
    "rcn",
    "network"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Ely Matos",
      "email": "admin@ematos.net"
    }
  ],
  "require": {
    "php": ">=7.1",
    "php-di/php-di": "^6.",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Net\\Ematos\\Mesh\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Net\\Ematos\\Mesh\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
